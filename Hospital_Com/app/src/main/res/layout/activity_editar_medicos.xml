<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditarMedicos">

    <EditText
        android:id="@+id/edttexPasswordEM"
        android:layout_width="270dp"
        android:layout_height="43dp"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="44dp"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="Digite una contraseña para su sesion"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/btnConfIrmarEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lblPasswordRM2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:fontFamily="serif"
        android:text="Contraseña"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/edttexPasswordEM"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lblTituloEM2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="146dp"
        android:layout_marginBottom="33dp"
        android:fontFamily="serif"
        android:text="Editar medico"
        android:textColor="#FF0000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/edttextNomEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edttextNomEM2"
        android:layout_width="173dp"
        android:layout_height="40dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="Editar nombre"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/edttextApeEM2"
        app:layout_constraintStart_toEndOf="@+id/lblNomEM2" />

    <TextView
        android:id="@+id/lblNomEM2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="34dp"
        android:fontFamily="serif"
        android:text="Nombres:"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/lblApeEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edttextApeEM2"
        android:layout_width="174dp"
        android:layout_height="41dp"
        android:layout_marginStart="39dp"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="Editar apellido"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/spEspecialidadEM2"
        app:layout_constraintStart_toEndOf="@+id/lblApeEM2" />

    <TextView
        android:id="@+id/lblApeEM2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="40dp"
        android:fontFamily="serif"
        android:text="Apellidos"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/lblEspEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spEspecialidadEM2"
        android:layout_width="176dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/edttextNumIdEM2"
        app:layout_constraintStart_toEndOf="@+id/lblEspEM2" />

    <TextView
        android:id="@+id/lblEspEM2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="23dp"
        android:fontFamily="serif"
        android:text="Especialidad"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/lblNumIdEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edttextNumIdEM2"
        android:layout_width="218dp"
        android:layout_height="38dp"
        android:layout_marginStart="9dp"
        android:layout_marginBottom="48dp"
        android:ems="10"
        android:fontFamily="serif"
        android:hint="Editar numero de Identificacion"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/spGeneroEM2"
        app:layout_constraintStart_toEndOf="@+id/lblNumIdEM2" />

    <TextView
        android:id="@+id/lblNumIdEM2"
        android:layout_width="94dp"
        android:layout_height="39dp"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="serif"
        android:text="Numero de Identificación"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/lblGeneroEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lblGeneroEM2"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="1dp"
        android:fontFamily="serif"
        android:text="Género"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/spGeneroEM2"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/spGeneroEM2"
        android:layout_width="130dp"
        android:layout_height="27dp"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="49dp"
        app:layout_constraintBottom_toTopOf="@+id/lblPasswordRM2"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnConfIrmarEM2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="144dp"
        android:layout_marginBottom="112dp"
        android:onClick="actualizar"
        android:text="Confirmar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>